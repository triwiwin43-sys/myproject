{
  "functions": {
    "frontend/dist/**": {
      "headers": {
        "cache-control": "s-maxage=0"
      }
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "cd frontend && rm -rf node_modules/.cache && rm -rf dist && npm ci && npm run build",
  "outputDirectory": "frontend/dist",
  "version": "2025-12-27-09-53-NO-CACHE"
}
